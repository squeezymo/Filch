<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_user_content_root_graph"
    app:startDestination="@id/userContentRootFragment">

    <dialog
        android:id="@+id/userContentRootFragment"
        android:name="me.squeezymo.usercontent.impl.ui.UserContentRootFragment"
        android:label="UserContentRootFragment">

        <argument
            android:name="from"
            app:argType="string" />

        <argument
            android:name="to"
            app:argType="string" />

        <argument
            android:name="autoMigrate"
            app:argType="boolean" />

        <deepLink
            app:uri="filch-a0eb0.firebaseapp.com://user_content?from={from}&amp;to={to}&amp;auto_migrate={autoMigrate}" />

    </dialog>

</navigation>
